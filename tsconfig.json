    "outDir": "./www/dist/",
    "noImplicitAny": true,
    "module": "AMD",
    "target": "ES6",
    "jsx": "react",
    "allowJs": true,
    "checkJs": true,
    "moduleResolution": "node",
    "esModuleInterop":true
  },
  "filesGlob": [
    "**/*.ts",
    "!node_modules/**"
  ],
}